{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "android": "ns run android --no-hmr",
    "ios": "ns run ios --no-hmr",
    "mobile": "ns run",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/app/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress": "concurrently \"ng serve\" \"cypress open\"",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,da,de,fi,nb,nl,sv}.json --clean --format json",
    "i18n:check-lint": "ngx-translate-lint -p ./src/app/**/*.{html,ts} -l ./src/assets/i18n/*.json",
    "i18n:check-keys": "ngx-translate-lint -p ./src/app/**/*.{html,ts} -v error",
    "format": "npx prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.2",
    "@angular/cdk": "~10.2.2",
    "@angular/common": "~10.2.2",
    "@angular/compiler": "~10.2.2",
    "@angular/core": "~10.2.2",
    "@angular/fire": "^6.1.1",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~10.2.2",
    "@angular/material": "~10.2.2",
    "@angular/platform-browser": "~10.2.2",
    "@angular/platform-browser-dynamic": "~10.2.2",
    "@angular/router": "~10.2.2",
    "@ionic/angular": "^5.5.2",
    "@ngrx/store": "^10.0.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "angular-material": "^1.2.1",
    "firebase": "^7.0.0 || ^8.0.0",
    "lodash": "^4.17.20",
    "material-design-icons": "^3.0.1",
    "ngx-translate-lint": "^1.7.3",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1200",
    "@angular-devkit/build-angular": "~0.1002.2",
    "@angular/cli": "~10.2.2",
    "@angular/compiler-cli": "~10.2.4",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@ionic/angular-toolkit": "^3.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.3.0",
    "cypress": "^6.1.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^6.0.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^7.0.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.0",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "main": "main.js"
}
